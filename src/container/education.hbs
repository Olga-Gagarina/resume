<div class="m-5 ">
  {{!-- "Education page in progress..." --}}
  <div>
    {{#with main}}
    <main>
      <section>
        {{>title data=" My educations" big=true}}
        <ul>
          {{#each education}}
          <li>{{name}}
            {{isEnd:false}}
            {{#unless isEnd}}
            (ще в процессі)
            {{/unless}}

          </li>
          {{/each}}
        </ul>
      </section>
      <section>
        {{>title data=" My certificates" big=false}}

        <ul>
          {{#each certificates}}
          <li>
            {{name}} ({{id}})
          </li>
          {{/each}}
        </ul>
      </section>


    </main>
    {{/with}}
  </div>
</div>
<div class="p-5">
    <main>
        {{#with header}}
        <h1> {{title}}</h1>
        <div class="row">
            <div class="col-5">
                <div class="mt-3 mb-5" mt-2> {{text}} </div>
            </div>
        </div>
        {{/with}}
        {{#with navigation}}
        {{#each buttons}}
        {{!-- {{text}} --}}
        <div class="m-2 ms-3 "> <a href="{{link}}" target="_blank" class="btn btn-primary text-uppercase">{{text}} </a>

        </div> {{!-- {{link}} --}}
        {{/each}}
        {{#each buttons_sm}}

        <div class="m-2 ms-3  "> <a href="{{link}}" target="_blank"
                class="btn btn-primary text-uppercase ps-5 pe-5 ">{{text}} </a>
        </div>

        {{/each}}
        {{/with}}
    </main>
</div>
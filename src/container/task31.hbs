<main class="m-5">
    <div class="row">
        <div class="col-8">
            {{#with navigation}}
            {{#each links}}
            <a href="{{href}}" class="btn btn-link" target="_blank"> {{text}} </a>
            {{/each}}

            <a href=" {{button.href}} " target="_blank" class="btn btn-primary ">{{button.text}} </a>
            {{/with}}
        </div>
        <section>
            <div class="card">
                <div class="card-body ">
                    {{#with header}}
                    <h1 class="col-3"> {{title}} </h1>
                    <p>{{description}}</p>
                    <a href=" {{button.link}}" target="_blank" class="btn btn-primary ">{{button.text}} </a>
                    {{/with}}
                </div>
            </div>
        </section>
        <section>
            <div class="card m-5 ">
                <div class="card-body">
                    {{#with city}}
                    <br>
                    <h2> {{title}} </h2>
                    <br>
                    <div class="row ">

                        {{#each cards}}
                        <li class=" card col-5 m-4 ">

                            <img class="card-img-top" src=" {{picture.src}} " alt=" {{picture.alt}}">
                            <div class="card-body">
                                <div class="card-title"> {{title}} </div>
                                <div class="card-body"> {{ description}} </div>

                                <a href=" {{button.href}} " target="_blank" class="btn btn-primary ">{{button.text}}</a>
                            </div>

                        </li>
                        {{/each}}
                        {{!--
                        </ul> --}}

                    </div>

                    {{/with}}
                </div>
            </div>
        </section>

        <section>

            {{#with winner}}
            <h2 class="ms-5"> {{title}} </h2>
            <div class="row ">
                <div class="col-12">
                    <table class="table m-5">
                        {{#with table}}
                        <thead>

                            <tr>
                                {{#each head}}
                                <th>
                                    {{this}}
                                </th>
                                {{/each}}
                            </tr>

                        </thead>
                        <tbody>
                            {{#each user}}
                            <tr>

                                <th>{{place}} </th>
                                <td>{{name}}</td>
                                <td>{{surname}}</td>
                                <td>{{nickname}}</td>


                            </tr>
                            {{/each}}
                        </tbody>
                        {{/with}}
                    </table>
                </div>
            </div>

            {{/with}}
        </section>
        <section>
            {{#with gallery}}
            <h2> {{title}} </h2>
            <div class="list-group">
                <div class="row ">

                    {{#each cards}}
                    <div class="card col-3 m-4 ">

                        <div class="card-header">
                            {{name}}
                        </div>

                        {{#each items}}

                        <div class="list-group-item">
                            {{this}}
                        </div>

                        {{/each}}

                    </div>
                    {{/each}}
                </div>
            </div>
            {{/with}}

        </section>
        <section>
            {{#with relaxion}}
            <div display-3> {{title}} </div>
            <div class="row col-12">

                {{#each cards}}
                <div class="card col-5 m-2 text-bg-secondary">
                    <div class="card header text-bg-secondary">{{header}} </div>
                    <div class=" card-body">
                        <div class="card-title text-secondary"> {{title}} </div>
                        <div> {{description}} </div>
                    </div>
                </div>
                {{/each}}

            </div>
            {{/with}}

        </section>
        <section>
            {{#with rest}}
            <div class="row">
                {{#each cards}}
                <div class="col-5">
                    <div class="card  mt-5">
                        <div class="card-header text-center "> {{header}} </div>
                        <div class="card-body text-center ">
                            <div class="card-title "> {{title}} </div>
                            <p> {{description}}</p>
                            <button class="btn btn-primary ">{{button}}</button>
                            <br>
                            <br>
                            <div class="card-footer text-center "> {{footer}}</div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
            {{/with}}


        </section>
        <section>
            {{#with proposition}}
            <div class="display-2 mt-4 mb-3"> {{title}} </div>
            <div class="row">
                {{#each card}}
                <div class="col-5">
                    <div class="card text-bg-warning">
                        <div class="card-header text-light">{{header}}</div>
                        <div class="card-body  ">
                            <h6 class="card-title text-secondary"> {{title}}</h6>
                            <p class="text-light">{{description}}</p>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>

            {{/with}}
        </section>




        <section class="mt-5">
            {{#with footer}}
            <h5> {{title}}
            </h5>
            <div class="row ">
                <div class="col-3">

                    {{#each navigation}}
                    <div>
                        <a href="{{href}}" target="_blank" class="btn btn-link">{{text}}</a><br><br>
                    </div>
                    {{/each}}



                </div>


                <div class=" col-3">

                    {{#each socialMedia}}
                    <div>
                        <a href="{{link}}" target="_blank" class="btn btn-outline-dark">{{name}}</a><br><BR>
                    </div>
                    {{/each}}

                </div>



                <div class=" col-3">

                    {{#each partners}}

                    <div>
                        <a href="{{link}}" target="_blank" class="btn btn-outline-info">{{name}}</a><br><br>
                    </div>

                    {{/each}}

                </div>

            </div>
            {{/with}}
        </section>

    </div>
</main>